{
  "name": "mcp-playground",
  "author": "Developer Jutra",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "start:docs": "node docs-server.js",
    "start:log:verbose": "CONFIG_LOG_LEVEL=VERBOSE node server.js",
    "test": "npx @modelcontextprotocol/inspector node server.js",
    "test:docs": "npx @modelcontextprotocol/inspector node docs-server.js",
    "test:cli": "npx @modelcontextprotocol/inspector --cli node server.js --method tools/list",
    "test:everything": "npx @modelcontextprotocol/inspector npx -y @modelcontextprotocol/server-everything"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.2",
    "zod": "^3.24.3"
  }
}
